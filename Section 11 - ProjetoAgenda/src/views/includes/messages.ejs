<% if(errors.length > 0) { %>
<div class="row">
  <div class="col">
    <div class="alert alert-danger text-center my-3">
      <% errors.forEach(erro => { %>
      <%= erro %><br>
      <% })%>
    </div>
  </div>
</div>
<% } %>

<% if(success.length > 0) { %>
<div class="row">
  <div class="col">
    <div class="alert alert-success text-center my-3">
      <% success.forEach(success => { %>
      <%= success %><br>
      <% })%>
    </div>
  </div>
</div>
<% } %>